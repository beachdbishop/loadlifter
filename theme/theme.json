{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"color": {
			"duotone": [
				{
					"slug": "gray",
					"colors": [ "#262626", "#fafafa" ],
					"name": "Gray"
				},
				{
					"slug": "red",
					"colors": [ "#940418", "#fef2f2" ],
					"name": "Red"
				},
				{
					"slug": "blue",
					"colors": [ "#092f42", "#b6ecff" ],
					"name": "Blue"
				}
			],
			"defaultDuotone": false,
			"gradients": [
				{
					"slug": "gray",
					"gradient": "linear-gradient(135deg, #525252, #525252 40%, #262626 95%, #262626)",
					"name": "Gray"
				},
				{
					"slug": "red",
					"gradient": "linear-gradient(135deg, #ce182d, #ce182d 40%, #940418 95%, #940418)",
					"name": "Red"
				},
				{
					"slug": "blue",
					"gradient": "linear-gradient(135deg, #00668e, #00668e 40%, #092f42 95%, #092f42)",
					"name": "Blue"
				},
				{
					"slug": "blue-faint",
					"gradient": "linear-gradient(135deg, #b6ecff, #b6ecff 40%, #00668e 95%, #00668e)",
					"name": "Blue Faint"
				},
				{
					"slug": "gray-split",
					"gradient": "linear-gradient(135deg, #525252, #525252 50%, #262626 50%, #262626)",
					"name": "Gray Split"
				},
				{
					"slug": "red-split",
					"gradient": "linear-gradient(135deg, #ce182d, #ce182d 50%, #940418 50%, #940418)",
					"name": "Red Split"
				},
				{
					"slug": "blue-split",
					"gradient": "linear-gradient(135deg, #00668e, #00668e 50%, #092f42 50%, #092f42)",
					"name": "Blue Split"
				},
				{
					"slug": "faint-red",
					"gradient": "linear-gradient(0deg, #fef2f2, #ffffff 40%, #ffffff)",
					"name": "Faint Red"
				},
				{
					"slug": "faint-blue",
					"gradient": "linear-gradient(0deg, #b6ecff, #ffffff 40%, #ffffff)",
					"name": "Faint Blue"
				},
				{
					"slug": "faint-gray",
					"gradient": "linear-gradient(0deg, #fafafa, #ffffff 40%, #ffffff)",
					"name": "Faint Gray"
				},
				{
					"slug": "eased-from-brand-blue-dark-to-brand-blue",
					"gradient": "linear-gradient(180deg, hsl(200deg 76% 15%) 0%, hsl(200deg 77% 16%) 8%, hsl(199deg 79% 17%) 17%, hsl(199deg 80% 18%) 25%, hsl(199deg 82% 19%) 33%, hsl(199deg 84% 21%) 42%, hsl(198deg 86% 22%) 50%, hsl(198deg 88% 23%) 58%, hsl(198deg 90% 24%) 67%, hsl(198deg 92% 25%) 75%, hsl(197deg 95% 26%) 83%, hsl(197deg 97% 27%) 92%, hsl(197deg 100% 28%) 100%)",
					"name": "Eased from Brand Blue Dark to Brand Blue"
				}
			],
			"defaultGradients": false,
			"palette": [
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "black",
					"color": "#000000",
					"name": "Black"
				},
				{
					"slug": "brand-gray-faint",
					"color": "#fafafa",
					"name": "Brand Gray Faint"
				},
				{
					"slug": "brand-gray-pale",
					"color": "#d4d4d4",
					"name": "Brand Gray Pale"
				},
				{
					"slug": "brand-gray",
					"color": "#525252",
					"name": "Brand Gray"
				},
				{
					"slug": "brand-gray-dark",
					"color": "#262626",
					"name": "Brand Gray Dark"
				},
				{
					"slug": "brand-red-faint",
					"color": "#fef2f2",
					"name": "Brand Red Faint"
				},
				{
					"slug": "brand-red-pale",
					"color": "#f87171",
					"name": "Brand Red Pale"
				},
				{
					"slug": "brand-red",
					"color": "#ce182d",
					"name": "Brand Red"
				},
				{
					"slug": "brand-red-dark",
					"color": "#940418",
					"name": "Brand Red Dark"
				},
				{
					"slug": "brand-blue-faint",
					"color": "#b6ecff",
					"name": "Brand Blue Faint"
				},
				{
					"slug": "brand-blue-pale",
					"color": "#2dcfff",
					"name": "Brand Blue Pale"
				},
				{
					"slug": "brand-blue",
					"color": "#00668e",
					"name": "Brand Blue"
				},
				{
					"slug": "orient-900",
					"color": "#075273",
					"name": "Orient 900"
				},
				{
					"slug": "brand-blue-dark",
					"color": "#092f42",
					"name": "Brand Blue Dark"
				}
			],
			"defaultPalette": false
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"textDecoration": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Serenity\", sans-serif",
					"name": "Serenity",
					"slug": "serenity",
					"fontFace": [
						{
							"fontFamily": "Serenity",
							"fontWeight": "300",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/serenity-light-webfont.woff2" ]
						},
						{
							"fontFamily": "Serenity",
							"fontWeight": "300",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./fonts/serenity-lightitalic-webfont.woff2" ]
						},
						{
							"fontFamily": "Serenity",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/serenity-bold-webfont.woff2" ]
						},
						{
							"fontFamily": "Serenity",
							"fontWeight": "600",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./fonts/serenity-bolditalic-webfont.woff2" ]
						}
					]
				}
			],
			"defaultFontSizes": false,
			"fontSizes": [
				{
					"name": "Extra Small",
					"size": "0.75rem",
					"fluid": {
						"min": "0.75rem",
						"max": "0.875rem"
					},
					"slug": "xs"
				},
				{
					"name": "Small",
					"size": "0.875rem",
					"fluid": {
						"min": "0.875rem",
						"max": "1rem"
					},
					"slug": "sm"
				},
				{
					"name": "Base",
					"size": "1rem",
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"slug": "base"
				},
				{
					"name": "Large",
					"size": "1.125rem",
					"fluid": {
						"min": "1.125rem",
						"max": "1.25rem"
					},
					"slug": "lg"
				},
				{
					"name": "Extra Large",
					"size": "1.25rem",
					"fluid": {
						"min": "1.25rem",
						"max": "1.5rem"
					},
					"slug": "xl"
				},
				{
					"name": "2 XL",
					"size": "1.5rem",
					"fluid": {
						"min": "1.5rem",
						"max": "1.875rem"
					},
					"slug": "2xl"
				},
				{
					"name": "3 XL",
					"size": "1.875rem",
					"fluid": {
						"min": "1.875rem",
						"max": "2.25rem"
					},
					"slug": "3xl"
				},
				{
					"name": "4 XL",
					"size": "2.25rem",
					"fluid": {
						"min": "2.25rem",
						"max": "2.875rem"
					},
					"slug": "4xl"
				}
			]
		},
		"layout": {
			"contentSize": "60rem",
			"wideSize": "80rem"
		},
		"spacing": {
			"blockGap": true
		}
	},
	"styles": {
		"blocks": {
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--3xl)"
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/query-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--2xl)"
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontSize": "var(--wp--preset--font-size--4xl)",
					"fontWeight": "normal"
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--4-xl)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--3-xl)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--2-xl)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--xl)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--lg)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"textTransform": "uppercase",
					"fontSize": "var(--wp--preset--font-size--base)"
				}
			},
			"button": {
				"border": {
					"radius": "8px",
					"color": "transparent"
				},
				"color": {
					"background": "var(--wp--preset--color--brand-blue-dark)",
					"text": "var(--wp--preset--color--brand-blue-faint)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"textDecoration": "none"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--brand-blue-dark)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--brand-blue-dark)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--brand-blue-dark)",
						"text": "var(--wp--preset--color--brand-blue-pale)"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--base)"
		}
	}
}
